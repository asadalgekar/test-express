<%- include("partials/header.ejs")%>
    <div class="cities-list">
        <h1>
            <% if (locals.dataOne) { %>
                <%= dataOne.name %> Cities
                    <% } else if (locals.renderCountrySessionData) { %>
                        <%= renderCountrySessionData.name %> Cities
                            <% } else if (locals.cachedMissedCountryData) { %>
                                <%= cachedMissedCountryData.name %> Cities
                                    <% } %>
        </h1>
        <div class="city-names">
            <table class="styled-table">
                <thead>
                    <tr>
                        <th>City Name</th>
                        <th>Population</th>
                    </tr>
                </thead>
                <tbody>
                    <% if (locals.dataTwo) { %>
                        <% dataTwo.forEach(data => { %>
                            <tr>
                                <td>
                                    <%= data.city %>
                                </td>
                                <td>
                                    <%= data.population %>
                                </td>
                            </tr>
                            <% }); %>
                                <% } else if (locals.renderCitySessionData) { %>
                                    <% renderCitySessionData.forEach(data => { %>
                                        <tr>
                                            <td> from session
                                                <%= data.city %>
                                            </td>
                                            <td>
                                                <%= data.population %>
                                            </td>
                                        </tr>
                                        <% }); %>
                                            <% } else if (locals.cachedMissedCityData) { %>
                                                <% cachedMissedCityData.forEach(data => { %>
                                                    <tr>
                                                        <td> from cached miss
                                                            <%= data.city %>
                                                        </td>
                                                        <td>
                                                            <%= data.population %>
                                                        </td>
                                                    </tr>
                                                    <% }); %>
                                                        <% } %>
                </tbody>
            </table>
        </div>
    </div>





    <h1>No cities cached data yet</h1>
    <form action="/distance/<%= name%>-<%= code %>" method="POST">
        <h2>Try for session data</h2>
        <button type="submit">Hit</button>
    </form>

    </body>

    </html>